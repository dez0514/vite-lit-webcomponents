{
  "name": "vite-lit-webcomponents",
  "version": "1.0.0",
  "author": "zwd",
  "main": "dist/index.es.js",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "keywords": [
    "webcomponents",
    "lit"
  ],
  "scripts": {
    "analyze": "custom-elements-manifest analyze --globs 'src/components/web-button.ts' --outdir dist",
    "dev": "vite",
    "build": "tsc && vite build && npm run copy && npm run docs",
    "copy": "cp src/index.html dist/",
    "preview": "vite preview",
    "docs": "npm run analyze && esmo scripts/docs.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "fs": "0.0.1-security",
    "lit": "^2.0.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@custom-elements-manifest/to-markdown": "^0.1.0",
    "@types/node": "^17.0.17",
    "esno": "^0.14.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2"
  },
  "customElements": "dist/custom-elements.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/dez0514/vite-lit-webcomponents"
  }
}
